{
  "name": "logsnap",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "npm run lint && vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build:css": "tailwind build src/assets/css/build.css -o src/assets/css/main.css",
    "build:ts": "tsc src/main.ts src/config/functions.ts"
  },
  "dependencies": {
    "@editorjs/editorjs": "^2.19.1",
    "@editorjs/header": "^2.6.1",
    "@tailwindcss/postcss7-compat": "^2.0.2",
    "@tinymce/tinymce-vue": "^4.0.0",
    "@uploadcare/upload-client": "^1.1.3",
    "animate.css": "^4.1.1",
    "axios": "^0.21.1",
    "ckeditor4-vue": "^1.3.0",
    "core-js": "^3.6.5",
    "firebase": "^8.2.6",
    "tiptap": "^1.32.1",
    "uuid": "^8.3.2",
    "vee-validate": "^4.1.18",
    "vue": "^3.0.0",
    "vue-3-meta": "^2.4.0",
    "vue-meta": "^2.4.0",
    "vue-multiselect": "^2.1.6",
    "vue-router": "^4.0.3",
    "vue2-editor": "^2.10.2",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "^4.5.11",
    "@vue/cli-plugin-typescript": "^4.5.11",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.11",
    "@vue/eslint-config-typescript": "^5.0.2",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0-0",
    "postcss": "^7.0.35",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.1.2",
    "typescript": "~3.9.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "@vue/typescript"
    ],
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "rules": {
      "no-unused-vars": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
